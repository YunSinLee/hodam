{
  "regions": ["icn1"],
  "functions": {
    "app/api/*": {
      "maxDuration": 60
    },
    "app/service": {
      "maxDuration": 60
    },
    "app/service/*": {
      "maxDuration": 60
    },
    "src/app/api/*": {
      "maxDuration": 60
    },
    "src/app/service": {
      "maxDuration": 60
    },
    "src/app/service/*": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "/sitemap-0.xml",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
